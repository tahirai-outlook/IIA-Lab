<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:common="using:AwareThings.WinIoTCoreServices.Core.Common"
    xmlns:common1="using:AwareThings.WinIoTCoreServices.Core.Common"
    xmlns:controls1="using:AwareThings.WinIoTCoreServices.Core.Panels.Controls"
    xmlns:controls2="using:AwareThings.WinIoTCoreServices.Core.Controls"
    xmlns:controls3="using:Microsoft.Toolkit.Uwp.UI.Controls">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="ms-appx:///Theme/Default/ApplicationTheme.xaml" />
    </ResourceDictionary.MergedDictionaries>
