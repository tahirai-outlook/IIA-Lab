            <TextBlock Text="{Binding DisplayValue}" Style="{StaticResource SkinMediumTextBlockStyle}" VerticalAlignment="Top" Foreground="{StaticResource SkinForegroundBrush}" Grid.Column="1" Grid.Row="1"  Margin="0,2,0,4" TextTrimming="CharacterEllipsis" TextWrapping="Wrap" TextLineBounds="Full"/>
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="DeviceInformationSkinPanelTemplate">
        <Border Style="{Binding SkinPanelOuterHostStyle}">
            <Grid Style="{Binding SkinPanelHostStyle}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <ContentControl Visibility="{Binding IsTitleShown, Converter={StaticResource VisibilityConverter}}" ContentTemplate="{Binding ItemHeaderDataTemplate}" Content="{Binding Title}" Grid.Row="0" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch"/>
                <ItemsControl ItemsSource="{Binding DisplayItems}" ItemTemplate="{StaticResource DeviceInformationItemLargeTemplate}" Grid.Row="1" Margin="10,10"/>
            </Grid>
        </Border>
    </DataTemplate>
    
    <DataTemplate x:Key="SpeedControlSkinPanelTemplate">
        <Border Style="{Binding SkinPanelOuterHostStyle}">
            <Grid Style="{Binding SkinPanelHostStyle}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <ContentControl Visibility="{Binding IsTitleShown, Converter={StaticResource VisibilityConverter}}" ContentTemplate="{Binding ItemHeaderDataTemplate}" Content="{Binding Title}" Grid.Row="0" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch"/>
                <controls1:SensorSpeedControl Value="{Binding Value, Mode=TwoWay}" Grid.Row="1" SpeedSettings="{Binding Settings}" PanelId="{Binding PanelId}" Margin="10"/>
            </Grid>
        </Border>
    </DataTemplate>

    <DataTemplate x:Key="MachineItemSkinPanelTemplate">
        <!--<Grid>
            <Grid.Resources>
                <ResourceDictionary>
                    <Style x:Key="SkinPanelOuterHostStyle" TargetType="Border" BasedOn="{Binding SkinPanelOuterHostStyle}">
                        <Setter Property="Background" Value="Red"/>
                    </Style>
                </ResourceDictionary>
            </Grid.Resources>-->
            <Border Style="{Binding SkinPanelOuterHostStyle}">
                <Grid Style="{Binding SkinPanelHostStyle}">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <ContentControl Visibility="{Binding IsTitleShown, Converter={StaticResource VisibilityConverter}}" ContentTemplate="{Binding ItemHeaderDataTemplate}" Content="{Binding Title}" Grid.Row="0" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch"/>
                    <TextBlock Text="{Binding MachineName}" Foreground="{StaticResource SkinForegroundBrush}" Style="{StaticResource SkinLargeHeaderTextBlockStyle}"  Margin="20,0,24,0" Grid.Row="1" />
                    <TextBlock Text="{Binding ModelNumber}" Foreground="{StaticResource SkinForeground75Brush}" Style="{StaticResource SkinMediumTextBlockStyle}" Margin="20,0"  Grid.Row="2"/>
                    <Grid BorderBrush="Transparent" BorderThickness="0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="0,10,0,0" Grid.Row="3">
                        <Image x:Name="MachineImage" common1:ImageExtensions.ImageUrl="{Binding MachineImageUri}" Stretch="Uniform" Margin="0" />
                    </Grid>
                </Grid>
            </Border>
        <!--</Grid>-->
    </DataTemplate>

    <DataTemplate x:Key="HeaderFooterValueItemTemplate">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <StackPanel Orientation="Horizontal" Grid.Row="1" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="20,0">
                <TextBlock Text="{Binding FormattedDisplayValue}" HorizontalAlignment="Left" FontWeight="Normal" Style="{StaticResource SkinLargeValueTextBlockStyle}" 
                           Foreground="{StaticResource SkinForegroundBrush}" Margin="4,0" Visibility="{Binding IsRunning, Converter={StaticResource VisibilityConverter}}"/>
            </StackPanel>
            <TextBlock Text="{Binding FormattedSymbol}" Style="{StaticResource SubtitleTextBlockStyle}" VerticalAlignment="Top" Grid.Row="1" HorizontalAlignment="Right" FontWeight="SemiBold" Margin="20,10" Foreground="{StaticResource SkinForegroundBrush}" />

            <TextBlock Text="{Binding Header}" Style="{StaticResource PanelHeaderTextBlockStyle}" Visibility="{Binding HasHeader, Converter={StaticResource VisibilityConverter}}" FontWeight="SemiBold" VerticalAlignment="Top" 
                       Grid.Row="0" HorizontalAlignment="Left" Margin="20,10,20,0" Foreground="{StaticResource SkinPrimaryAccentBrush}"/>
            <TextBlock Text="{Binding Footer}" Foreground="{StaticResource SkinForeground75Brush}" Style="{StaticResource DetailsValueExtraSmallTextBlockStyle}" Visibility="{Binding HasFooter, Converter={StaticResource VisibilityConverter}}" VerticalAlignment="Top" 
                       Grid.Row="2" HorizontalAlignment="Left" Margin="20,0,20,10"/>
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="SingleValueSkinPanelTemplate">
        <Border Style="{Binding SkinPanelOuterHostStyle}">
            <Grid Style="{Binding SkinPanelHostStyle}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <ContentControl Visibility="{Binding IsTitleShown, Converter={StaticResource VisibilityConverter}}" ContentTemplate="{Binding ItemHeaderDataTemplate}" Content="{Binding Title}" Grid.Row="0" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch"/>
                <ContentControl ContentTemplate="{StaticResource HeaderFooterValueItemTemplate}" Content="{Binding}" Grid.Row="1" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch"/>
                <ContentPresenter Grid.Row="0" Grid.RowSpan="2" ContentTemplate="{StaticResource SensorInitializingOverlayTemplate}" Content="{Binding}" Visibility="{Binding IsInitializing, Converter={StaticResource VisibilityConverter}}"/>
                <ContentPresenter Grid.Row="0" Grid.RowSpan="2" ContentTemplate="{StaticResource SensorFailedOverlayTemplate}" Content="{Binding}" Visibility="{Binding IsFailed, Converter={StaticResource VisibilityConverter}}"/>
            </Grid>
        </Border>
    </DataTemplate>

    <DataTemplate x:Key="ChartSensorSkinPanelTemplate">
        <Border Style="{Binding SkinPanelOuterHostStyle}">
            <Grid Style="{Binding SkinPanelHostStyle}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <ContentControl Visibility="{Binding IsTitleShown, Converter={StaticResource VisibilityConverter}}" ContentTemplate="{Binding ItemHeaderDataTemplate}" Content="{Binding Title}" Grid.Row="0" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch" Grid.ColumnSpan="2"/>
                <ContentControl Grid.ColumnSpan="{Binding ChartColumnSpan}" ContentTemplate="{StaticResource HeaderFooterValueItemTemplate}" Content="{Binding}" Grid.Row="1" HorizontalAlignment="Left" MinWidth="200" HorizontalContentAlignment="Stretch"/>
                <Grid Grid.Row="{Binding ChartRow}" Grid.Column="{Binding ChartColumn}" Grid.ColumnSpan="{Binding ChartColumnSpan}">
                    <controls1:SimpleGraphControl ChartValues="{Binding ChartValues}" ChartSettings="{Binding Settings}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"
                                                  GraphBarBrush="{StaticResource SkinPrimaryAccentBrush}" GraphFillBrush="{StaticResource SkinPrimaryAccentDarkBrush}" GraphLineBrush="{StaticResource SkinPrimaryAccentBrush}"/>
                </Grid>
                <ContentPresenter Grid.Row="0" Grid.RowSpan="3" Grid.ColumnSpan="2" ContentTemplate="{StaticResource SensorInitializingOverlayTemplate}" Content="{Binding}" Visibility="{Binding IsInitializing, Converter={StaticResource VisibilityConverter}}"/>
                <ContentPresenter Grid.Row="0" Grid.RowSpan="3" Grid.ColumnSpan="2" ContentTemplate="{StaticResource SensorFailedOverlayTemplate}" Content="{Binding}" Visibility="{Binding IsFailed, Converter={StaticResource VisibilityConverter}}"/>
            </Grid>
        </Border>
    </DataTemplate>


    <DataTemplate x:Key="MultiValueSkinPanelTemplate">
        <Border Style="{Binding SkinPanelOuterHostStyle}">
            <Grid Style="{Binding SkinPanelHostStyle}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <ContentControl Visibility="{Binding IsTitleShown, Converter={StaticResource VisibilityConverter}}" ContentTemplate="{Binding ItemHeaderDataTemplate}" Content="{Binding Title}" Grid.Row="0" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch"/>
                <StackPanel Orientation="Vertical" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Stretch" Margin="24,0" Width="60">
                    <TextBlock Text="{Binding Symbol}" Style="{StaticResource DetailsHeaderTextBlockStyle}" Margin="0" FontWeight="SemiBold"/>
                    <TextBlock Text="{Binding DisplayValue}" Style="{StaticResource DetailsValueLargeTextBlockStyle}" Foreground="{StaticResource SkinForegroundBrush}" Margin="4,0" Visibility="{Binding IsRunning, Converter={StaticResource VisibilityConverter}}"/>
                </StackPanel>
            </Grid>
        </Border>
    </DataTemplate>

    <DataTemplate x:Key="ProvisioningInfoItemLargeTemplate">
        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="10,10,10,20" Width="160">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <TextBlock Text="{Binding Label}" Style="{StaticResource DetailsValueMediumTextBlockStyle}" HorizontalAlignment="Stretch" Foreground="{StaticResource SkinForegroundBrush}" FontWeight="Normal" VerticalAlignment="Top" Grid.Row="0" Grid.Column="1" Margin="0,-6,0,0" TextTrimming="CharacterEllipsis" TextLineBounds="Full"/>
            <TextBlock Text="{Binding DisplayValue}" Style="{StaticResource DetailsValueLargeTextBlockStyle}" Foreground="{StaticResource SkinForegroundBrush}" VerticalAlignment="Top" Grid.Column="1" Grid.Row="1"  Margin="0,2,0,4" TextTrimming="CharacterEllipsis" TextWrapping="Wrap" TextLineBounds="Full" FontWeight="SemiBold"/>
            <!--<Rectangle Height="1" Fill="{StaticResource Gray4Brush}" HorizontalAlignment="Stretch" Grid.Row="2" Grid.ColumnSpan="2" Grid.Column="0" VerticalAlignment="Bottom" Margin="0,0,0,-4"/>-->
        </Grid>
    </DataTemplate>


    <DataTemplate x:Key="ProvisioningInfoPanelTemplate">
        <Grid Style="{Binding SkinPanelHostStyle}">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <ContentControl Visibility="{Binding IsTitleShown, Converter={StaticResource VisibilityConverter}}" ContentTemplate="{Binding ItemHeaderDataTemplate}" Content="{Binding Title}" Grid.Row="0" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch"/>
            <GridView ItemsSource="{Binding DisplayItems}" ItemTemplate="{StaticResource ProvisioningInfoItemLargeTemplate}" Grid.Row="1" Margin="10,10" VerticalAlignment="Center"/>
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="ProvisioningDetailsPanelTemplate">
        <Grid Style="{Binding SkinPanelHostStyle}" Margin="20">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <TextBlock Text="{Binding StepTitle}" Style="{StaticResource SkinLargeHeaderTextBlockStyle}" HorizontalAlignment="Stretch" Foreground="{StaticResource SkinForegroundBrush}" FontWeight="SemiBold" VerticalAlignment="Top" Grid.Row="0" Margin="0,8,0,0" TextTrimming="CharacterEllipsis" TextLineBounds="Full"/>
            <TextBlock Text="{Binding StepDescription}" Style="{StaticResource SkinMediumTextBlockStyle}" VerticalAlignment="Top" Foreground="{StaticResource SkinForeground75Brush}" Grid.Row="1"  Margin="0,2,0,4" TextTrimming="CharacterEllipsis" TextWrapping="Wrap" TextLineBounds="Full"/>
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="ProvisioningActionPanelTemplate">
        <Grid Style="{Binding SkinPanelHostStyle}" Margin="20">
        </Grid>
    </DataTemplate>


    <DataTemplate x:Key="ProvisioningProgressPanelTemplate">
        <Grid Style="{Binding SkinPanelHostStyle}" Margin="20">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <TextBlock Text="{Binding Title}" Style="{StaticResource PageSubHeaderTextBlockStyle}" Foreground="{StaticResource SkinForegroundBrush}" Margin="0,20,0,0" FontWeight="SemiBold"/>
            <ItemsControl ItemsSource="{Binding StatusItems}" Grid.Row="1" Margin="0,40,0,0">
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <Grid Height="60" BorderThickness="0,0,0,0" BorderBrush="#44FFFFFF" Margin="0,4">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="60"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Orientation="Vertical" Grid.Column="1">
                                <TextBlock Text="{Binding Title}" Style="{StaticResource DetailsValueLargeTextBlockStyle}" Foreground="{StaticResource SkinForegroundBrush}" Margin="0" FontWeight="SemiLight" Opacity="0.5" VerticalAlignment="Center" Visibility="{Binding IsIdle, Converter={StaticResource VisibilityConverter}}"/>
                                <TextBlock Text="{Binding Title}" Style="{StaticResource DetailsValueLargeTextBlockStyle}" Foreground="{StaticResource SkinForegroundBrush}" Margin="0" FontWeight="Normal" VerticalAlignment="Center" Visibility="{Binding IsIdle, Converter={StaticResource InverseVisibilityConverter}}"/>
                                <TextBlock Text="{Binding Detail}" Style="{StaticResource DetailsValueExtraSmallTextBlockStyle}" Foreground="{StaticResource SkinForegroundBrush}" Margin="0,5,0,0" FontWeight="SemiBold" HorizontalAlignment="Left" VerticalAlignment="Center" TextLineBounds="Tight"/>
                            </StackPanel>
                            <Grid Width="32" Height="32" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,0,0">
                                <Path Data="M22.899994,10.699997L25.099991,13.199997 13.5,23.399994 7,15.899994 9.5,13.699997 13.799988,18.699997z M16,3.5999908C9.1999969,3.5999908 3.5999908,9.1999969 3.5999908,16 3.5999908,18.699997 4.3999939,21.099991 5.8999939,23.199997L6,23.300003 6.0999908,23.399994C6.5,23.899994,6.7999878,24.300003,7.2999878,24.699997L7.5,24.899994 7.6999969,24.899994 7.6999969,25 7.7999878,25.099991C10,27.099991 12.899994,28.300003 16.099991,28.300003 22.899994,28.300003 28.5,22.800003 28.5,15.899994 28.399994,9.1999969 22.799988,3.5999908 16,3.5999908z M16,0C24.799988,0 32,7.1999969 32,16 32,24.800003 24.799988,32 16,32 7.1999969,32 0,24.800003 0,16 0,7.1999969 7.1999969,0 16,0z" Stretch="Uniform" Fill="{StaticResource SkinForegroundBrush}" Width="32" Height="32" Margin="0,0,0,0" RenderTransformOrigin="0.5,0.5"
                                      Visibility="{Binding IsConnected, Converter={StaticResource VisibilityConverter}}"/>
                                <Ellipse Fill="{StaticResource SkinErrorAccentBrush}" Visibility="{Binding IsConnectionFailed, Converter={StaticResource VisibilityConverter}}"/>
                                <Ellipse Fill="{StaticResource SkinAlertAccentBrush}" Visibility="{Binding IsWarning, Converter={StaticResource VisibilityConverter}}" Opacity="0.7"/>
                                <!--<Ellipse Fill="{StaticResource Gray4Brush}" Visibility="{Binding IsIdle, Converter={StaticResource VisibilityConverter}}"/>
                                <Ellipse Fill="Red" Visibility="{Binding IsWarning, Converter={StaticResource VisibilityConverter}}" Opacity="0.7"/>-->
                            </Grid>
                            <ProgressRing Foreground="{StaticResource SkinForegroundBrush}" IsActive="True" Grid.Column="0" Width="38" Height="38" HorizontalAlignment="Left" Margin="0,6,0,0" VerticalAlignment="Top" Visibility="{Binding IsConnecting, Converter={StaticResource VisibilityConverter}}"/>
                        </Grid>
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ItemsControl>
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="ProvisioningLogPanelTemplate">
        <Grid/>
    </DataTemplate>

    <DataTemplate x:Key="MarkdownReaderPanelTemplate">
        <Border Style="{Binding SkinPanelOuterHostStyle}">
            <Grid Style="{Binding SkinPanelHostStyle}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <ContentControl Visibility="{Binding IsTitleShown, Converter={StaticResource VisibilityConverter}}" ContentTemplate="{Binding ItemHeaderDataTemplate}" Content="{Binding Title}" Grid.Row="0" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch"/>
                <ScrollViewer VerticalScrollMode="Enabled" HorizontalScrollMode="Disabled" Padding="20" Grid.Row="1">
                    <controls3:MarkdownTextBlock Text="{Binding MarkdownText}" UriPrefix="{Binding BasePathUri}" Background="Transparent" RequestedTheme="{Binding RequestedTheme}" />
                </ScrollViewer>
            </Grid>
        </Border>
    </DataTemplate>

    <DataTemplate x:Key="CustomContentPanelTemplate">
        <Border Child="{Binding CustomElement}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
    </DataTemplate>

    <DataTemplate x:Key="ImagePanelTemplate">
        <Grid BorderBrush="Transparent" BorderThickness="0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="0,0,0,0">
            <Image x:Name="MachineImage" common1:ImageExtensions.ImageUrl="{Binding ImageUri}" Stretch="{Binding Stretch}" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center" />
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="FlightRingSkinPanelTemplate">
        <Border Style="{Binding SkinPanelOuterHostStyle}">
            <Grid Style="{Binding SkinPanelHostStyle}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <ContentControl Visibility="{Binding IsTitleShown, Converter={StaticResource VisibilityConverter}}" ContentTemplate="{Binding ItemHeaderDataTemplate}" Content="{Binding Title}" Grid.Row="0" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch"/>
                <StackPanel Orientation="Vertical" Grid.Row="1" Margin="24,20,24,10" >
                    <TextBlock Text="Current Version" Style="{StaticResource DetailsHeaderTextBlockStyle}"/>
                    <TextBlock Text="{Binding FlightRingName}" Style="{StaticResource DetailsValueLargeTextBlockStyle}" />
                </StackPanel>
                <StackPanel Orientation="Vertical" Grid.Row="2"  Margin="24,10" >
                    <TextBlock Text="Last Installed" Style="{StaticResource DetailsHeaderTextBlockStyle}"/>
                    <TextBlock Text="{Binding LastInstalled}" Style="{StaticResource DetailsValueLargeTextBlockStyle}"  />
                </StackPanel>
                <StackPanel Orientation="Vertical" Grid.Row="3"  Margin="24,10">
                    <TextBlock Text="Current State" Style="{StaticResource DetailsHeaderTextBlockStyle}"/>
                    <TextBlock Text="Idle" Style="{StaticResource DetailsValueLargeTextBlockStyle}"  Visibility="{Binding IsIdle, Converter={StaticResource VisibilityConverter}}"/>
                    <TextBlock Text="Checking" Style="{StaticResource DetailsValueLargeTextBlockStyle}"  Foreground="{StaticResource SkinPrimaryAccentBrush}" Visibility="{Binding IsChecking, Converter={StaticResource VisibilityConverter}}"/>
                    <ProgressBar HorizontalAlignment="Stretch" Foreground="{StaticResource CyanAccentBrush}" Margin="0,10,0,0" IsIndeterminate="{Binding IsChecking}"  Visibility="{Binding IsChecking, Converter={StaticResource VisibilityConverter}}"/>
                    <TextBlock Text="Up to Date" Style="{StaticResource DetailsValueLargeTextBlockStyle}"  Visibility="{Binding IsUpToDate, Converter={StaticResource VisibilityConverter}}"/>
                    <TextBlock Text="Downloading" Style="{StaticResource DetailsValueLargeTextBlockStyle}" Foreground="{StaticResource SkinPrimaryAccentBrush}"  Visibility="{Binding IsDownloading, Converter={StaticResource VisibilityConverter}}"/>
                    <ProgressBar HorizontalAlignment="Stretch" Foreground="{StaticResource CyanAccentBrush}" Margin="0,10,0,0" IsIndeterminate="{Binding IsDownloading}"  Visibility="{Binding IsDownloading, Converter={StaticResource VisibilityConverter}}"/>
                    <TextBlock Text="Installing" Style="{StaticResource DetailsValueLargeTextBlockStyle}"  Visibility="{Binding IsInstalling, Converter={StaticResource VisibilityConverter}}"/>
                    <ProgressBar HorizontalAlignment="Stretch" Foreground="{StaticResource SkinForegroundBrush}" Margin="0,10,0,0" IsIndeterminate="{Binding IsInstalling}"  Visibility="{Binding IsInstalling, Converter={StaticResource VisibilityConverter}}"/>
                    <TextBlock Text="Reboot Required" Style="{StaticResource DetailsValueLargeTextBlockStyle}" Foreground="{StaticResource SkinPrimaryAccentBrush}"  Visibility="{Binding IsRebootRequired, Converter={StaticResource VisibilityConverter}}"/>
                </StackPanel>
            </Grid>
        </Border>
    </DataTemplate>

    <controls2:FlexibleLayoutContentTemplateSelector x:Key="SkinTemplateSelector"
                                                    ConnectionStatusPanelTemplate="{StaticResource ConnectionsSkinPanelTemplate}"
                                                    FlightRingPanelTemplate="{StaticResource FlightRingSkinPanelTemplate}"
                                                    GaugeSensorPanelTemplate="{StaticResource GaugeSensorSkinPanelTemplate}"
                                                    LogItemsPanelTemplate="{StaticResource LogSkinPanelTemplate}"
                                                    MachinePanelTemplate="{StaticResource MachineItemSkinPanelTemplate}"
                                                    SpeedControlPanelTemplate="{StaticResource SpeedControlSkinPanelTemplate}"
                                                    VibrationSensorPanelTemplate="{StaticResource VibrationSensorSkinPanelTemplate}"
                                                    DeviceInformationPanelTemplate="{StaticResource DeviceInformationSkinPanelTemplate}"
                                                    SingleValueSensorPanelTemplate="{StaticResource SingleValueSkinPanelTemplate}"
                                                    MultiValueSensorPanelTemplate="{StaticResource MultiValueSkinPanelTemplate}"
                                                    ChartSensorPanelTemplate="{StaticResource ChartSensorSkinPanelTemplate}"
                                                    SensorStatusPanelTemplate="{StaticResource SensorStatusSkinPanelTemplate}"
                                                     CustomContentPanelTemplate="{StaticResource CustomContentPanelTemplate}"
                                                    ImagePanelTemplate="{StaticResource ImagePanelTemplate}"
                                                    MarkdownReaderPanelTemplate="{StaticResource MarkdownReaderPanelTemplate}"
                                                    ProvisioningInfoPanelTemplate="{StaticResource ProvisioningInfoPanelTemplate}"
                                                    ProvisioningLogPanelTemplate="{StaticResource ProvisioningLogPanelTemplate}"
                                                    ProvisioningProgressPanelTemplate="{StaticResource ProvisioningProgressPanelTemplate}"
                                                    ProvisioningActionPanel="{StaticResource ProvisioningActionPanelTemplate}"
                                                    ProvisioningDetailsPanel="{StaticResource ProvisioningDetailsPanelTemplate}"
                                                    SliderSensorPanelTemplate="{StaticResource SliderSensorSkinPanelTemplate}"
    />

    <controls2:FlexibleHeaderTemplateSelector x:Key="HeaderTemplateSelector"
                                              DefaultTemplate="{StaticResource DefaultHeaderTemplate}"
                                              DefaultTemplate1="{StaticResource DefaultHeaderTemplate1}"
                                              DefaultTemplate2="{StaticResource DefaultHeaderTemplate2}"
                                              />

</ResourceDictionary>
