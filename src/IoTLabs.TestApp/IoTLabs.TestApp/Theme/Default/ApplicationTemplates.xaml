    <DataTemplate x:Key="GaugeSensorSkinPanelTemplate">
        <Border Style="{Binding SkinPanelOuterHostStyle}">
            <Grid Style="{Binding SkinPanelHostStyle}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <ContentControl Visibility="{Binding IsTitleShown, Converter={StaticResource VisibilityConverter}}" 
                                ContentTemplate="{Binding ItemHeaderDataTemplate}" Content="{Binding Title}" Grid.Row="0" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch" Grid.ColumnSpan="2"/>
                
                <controls1:GaugeSensorControl Value="{Binding Value}" Symbol="{Binding Symbol}" 
                                              DoubleSettings="{Binding Settings}" 
                                              VerticalAlignment="Top"
                                              IsRunning="{Binding IsRunning}"
                                              AlertRangeColor="{StaticResource SkinSecondaryAccentLightBrush}"
                                              RangeColor="{StaticResource SkinForeground50Brush}"
                                              IdealRangeColor="{StaticResource SkinSecondaryAccentBrush}"
                                              NeedleColor="{StaticResource SkinForegroundBrush}"
                                              TickColor="{StaticResource SkinForeground75Brush}"
                                              Grid.Row="{Binding GaugeRow}"
                                              Grid.ColumnSpan="{Binding GaugeColumnSpan}"
                                              Grid.Column="{Binding GaugeColumn}" Margin="10,10,10,0"
                                              IsNeedleMode="{Binding IsNeedleMode}"
                                              IsCompressedMode="{Binding IsCompressedMode}"/>

                <ContentControl ContentTemplate="{StaticResource HeaderFooterValueItemTemplate}" Grid.Row="1" Margin="0,0,0,0" 
                                Visibility="Visible" VerticalAlignment="Top" MinWidth="200" Grid.ColumnSpan="{Binding GaugeColumnSpan}"
                                Content="{Binding}" Grid.Column="0" HorizontalAlignment="Left" HorizontalContentAlignment="Stretch"/>

                <ContentPresenter Grid.Row="0" Grid.RowSpan="3" Grid.ColumnSpan="2" ContentTemplate="{StaticResource SensorInitializingOverlayTemplate}" Content="{Binding}" Visibility="{Binding IsInitializing, Converter={StaticResource VisibilityConverter}}"/>
                <ContentPresenter Grid.Row="0" Grid.RowSpan="3" Grid.ColumnSpan="2" ContentTemplate="{StaticResource SensorFailedOverlayTemplate}" Content="{Binding}" Visibility="{Binding IsFailed, Converter={StaticResource VisibilityConverter}}"/>
            </Grid>
        </Border>
    </DataTemplate>

    <DataTemplate x:Key="SliderSensorSkinPanelTemplate">
        <Border Style="{Binding SkinPanelOuterHostStyle}">
            <Grid Style="{Binding SkinPanelHostStyle}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="200"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <ContentControl Visibility="{Binding IsTitleShown, Converter={StaticResource VisibilityConverter}}" ContentTemplate="{Binding ItemHeaderDataTemplate}" Content="{Binding Title}" Grid.Row="0" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch"
                                Grid.ColumnSpan="2" />
                <!--<controls1:GaugeSensorControl Value="{Binding Value}" Symbol="{Binding Symbol}" DoubleSettings="{Binding Settings}" 
                                              IsRunning="{Binding IsRunning}"
                                              Grid.Row="1" Margin="20,20,20,0"/>-->

                <ContentControl ContentTemplate="{StaticResource HeaderFooterValueItemTemplate}" Content="{Binding}" Grid.Row="1" Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Stretch" 
                                HorizontalContentAlignment="Stretch" 
                                Margin="0,0,0,4"/>
                <controls1:SliderSensorControl Value="{Binding Value}" Settings="{Binding Settings}" Grid.Row="1" Grid.Column="1" Margin="10,14,30,14" 
                                               Height="30" 
                                               VerticalAlignment="Center"
                                               RangeColor="{StaticResource SkinSecondaryAccentDarkBrush}"
                                               GaugeColor="{StaticResource SkinPrimaryAccentBrush}"
                                               IdealColor="{StaticResource SkinSecondaryAccentBrush}"
                                               
                                               />
                <ContentPresenter Grid.Row="0" Grid.RowSpan="2" Grid.ColumnSpan="2" ContentTemplate="{StaticResource SensorInitializingOverlayTemplate}" Content="{Binding}" Visibility="{Binding IsInitializing, Converter={StaticResource VisibilityConverter}}"/>
                <ContentPresenter Grid.Row="0" Grid.RowSpan="2" Grid.ColumnSpan="2" ContentTemplate="{StaticResource SensorFailedOverlayTemplate}" Content="{Binding}" Visibility="{Binding IsFailed, Converter={StaticResource VisibilityConverter}}"/>
            </Grid>
        </Border>
    </DataTemplate>

    <DataTemplate x:Key="VibrationSensorSkinPanelTemplate">
        
            <Border Style="{Binding SkinPanelOuterHostStyle}">
