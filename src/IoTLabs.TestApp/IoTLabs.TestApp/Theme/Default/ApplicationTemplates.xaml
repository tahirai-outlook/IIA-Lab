                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <ContentControl Visibility="{Binding IsTitleShown, Converter={StaticResource VisibilityConverter}}" ContentTemplate="{Binding ItemHeaderDataTemplate}" Content="{Binding Title}" Grid.Row="0" Grid.ColumnSpan="2" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch"/>

                <!--<StackPanel Grid.Column="1" Orientation="Vertical" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Stretch" 
