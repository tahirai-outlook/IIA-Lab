    <common:VisibilityConverter x:Key="VisibilityConverter"/>
    <common:InverseVisibilityConverter x:Key="InverseVisibilityConverter"/>

    <DataTemplate x:Key="DefaultHeaderTemplate2">
        <Grid Height="100" Background="{StaticResource SkinPageHeaderBackgroundBrush}">
            <StackPanel Orientation="Horizontal" Margin="24,10,0,10">
                <Border Width="90" Height="90">
                    <Image common:ImageExtensions.ImageUrl="{Binding HeaderIcon}"/>
                </Border>
                <StackPanel Orientation="Vertical" VerticalAlignment="Center" Margin="0,0,0,0">
                    <TextBlock Grid.Column="0" Style="{StaticResource SkinMediumTextBlockStyle}" Foreground="{StaticResource SkinPageHeaderForegroundBrush}"
                               Margin="16,8,0,0" Text="{Binding PageHeader, FallbackValue=Acme Incorporated}" Opacity="0.7"
                               VerticalAlignment="Center" HorizontalAlignment="Left" Padding="0" TextLineBounds="Tight"/>
                    <TextBlock Grid.Column="0" Style="{StaticResource SkinLargeHeaderTextBlockStyle}" Foreground="{StaticResource SkinPageHeaderForegroundBrush}"
                               Margin="16,4,0,0"  Text="{Binding PageSubHeader, FallbackValue=IoT Core Services}" 
                               VerticalAlignment="Center" HorizontalAlignment="Left" Padding="0"/>
                </StackPanel>
            </StackPanel>
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="DefaultHeaderTemplate">
        <Grid Height="110" Background="{StaticResource SkinPageHeaderBackgroundBrush}">
            <StackPanel Orientation="Horizontal" Margin="0,0,0,0">
                <Border Width="120" Padding="10">
                    <Image common:ImageExtensions.ImageUrl="{Binding HeaderIcon}"/>
                </Border>
                <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Margin="0,0,0,0">
                    <TextBlock Grid.Column="0" Style="{StaticResource SkinLargeHeaderTextBlockStyle}" FontSize="48" Foreground="{StaticResource SkinPageHeaderForegroundBrush}" Margin="16,8,0,0" Text="{Binding PageHeader, FallbackValue=Acme Incorporated}" VerticalAlignment="Center" HorizontalAlignment="Left" Padding="0" TextLineBounds="TrimToCapHeight" FontWeight="SemiBold"/>
                    <Rectangle Width="1" Fill="{StaticResource SkinPageHeaderForegroundBrush}" Margin="20,-8"/>
                    <TextBlock Grid.Column="0" Style="{StaticResource SkinLargeHeaderTextBlockStyle}" FontSize="48" Foreground="{StaticResource SkinPageHeaderForegroundBrush}" Margin="0,8,0,0"  Text="{Binding PageSubHeader, FallbackValue=IoT Core Services}" VerticalAlignment="Center" HorizontalAlignment="Left" Padding="0"  TextLineBounds="TrimToCapHeight" FontWeight="Normal"/>
                </StackPanel>
            </StackPanel>
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="DefaultHeaderTemplate1">
        <Grid Height="110" Background="{StaticResource SkinPageHeaderBackgroundBrush}">
            <StackPanel Orientation="Horizontal" Margin="0,0,0,0">
                <Border Padding="10">
                    <Image common:ImageExtensions.ImageUrl="{Binding HeaderIcon}" Stretch="Uniform"/>
                </Border>
                <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Margin="0,0,0,0">
                    <TextBlock Grid.Column="0" Style="{StaticResource SkinLargeHeaderTextBlockStyle}" FontSize="48" Foreground="{StaticResource SkinPageHeaderForegroundBrush}" Margin="0,8,0,0"  Text="{Binding PageSubHeader, FallbackValue=IoT Core Services}" VerticalAlignment="Center" HorizontalAlignment="Left" Padding="0"  TextLineBounds="TrimToCapHeight" FontWeight="Normal"/>
                </StackPanel>
            </StackPanel>
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="SensorInitializingOverlayTemplate">
        <Grid>
            <Border Background="{StaticResource SkinPanelBackgroundBrush}" Opacity="0.8"/>
            <Border Background="{StaticResource SkinPanelInverseBackgroundBrush}" VerticalAlignment="Center" HorizontalAlignment="Center" 
                    Padding="20,8" CornerRadius="4" BorderThickness="1" BorderBrush="{StaticResource SkinPanelInverseHeaderBrush}">
                <TextBlock Text="Initializing" Style="{StaticResource SkinMediumTextBlockStyle}" FontWeight="SemiBold" Foreground="{StaticResource SkinPanelInverseHeaderBrush}" HorizontalAlignment="Center" VerticalAlignment="Center" TextLineBounds="Tight"/>
            </Border>
        </Grid>
    </DataTemplate>

    <DataTemplate x:Key="SensorFailedOverlayTemplate">
        <Grid Background="#00000000">
            <Border Background="Red" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="20,8" CornerRadius="4">
                <TextBlock Text="Sensor Failed" Style="{StaticResource SubtitleTextBlockStyle}" FontWeight="SemiBold" Foreground="{StaticResource SkinForegroundBrush}" HorizontalAlignment="Center" VerticalAlignment="Center" TextLineBounds="Tight"/>
            </Border>
        </Grid>
    </DataTemplate>


